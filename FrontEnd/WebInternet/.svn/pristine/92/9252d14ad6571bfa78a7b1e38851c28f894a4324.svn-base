@font-face {
    font-family: 'jca';
    src: url('../fonts/jca.eot?8p3bjg');
    src: url('../fonts/jca.eot?8p3bjg#iefix') format('embedded-opentype'), 
    url('../fonts/jca.ttf?8p3bjg') format('truetype'), 
    url('../fonts/jca.woff?8p3bjg') format('woff'), 
    url('../fonts/jca.svg?8p3bjg#jca') format('svg');
    font-weight: normal;
    font-style: normal;
}

.jca {
    display: inline-block;
    font: normal normal normal 14px/1 jca;
    font-size: inherit;
    text-rendering: auto;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}

.jca-lg {
    font-size: 1.33333333em;
    line-height: 0.75em;
    vertical-align: -15%;
}

.jca-2x {
    font-size: 2em;
}

.jca-3x {
    font-size: 3em;
}

.jca-4x {
    font-size: 4em;
}

.jca-5x {
    font-size: 5em;
}

